<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<bean id="empl" class="com.springcore2.Employee" scope="prototype">
<!-- scope can be singleton(default)  -->
<property name="employeeId" value="123"/>
<property name="employeeName" value="prem"/>
<property name="salary" value="25000"/>
<!-- <property name="businessUnit" value="Product Development"/>  -->
</bean>
<bean id="Sbu" class="com.springcore2.SBU" scope="prototype">
<property name="sbuId" value="120"/>
<property name="sbuName" value="Product Development"/>
<property name="subHead" value="Ramprasad"/>
</bean>

</beans>